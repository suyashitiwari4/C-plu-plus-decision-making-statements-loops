#include<iostream>
using namespace std;
int main(){
    int i,j;
    int n=5;

    for(i=1;i<=n;i++){
        
        for(j=1;j<=n-i;j++){
            cout<<" " ;
          
        }
        for(int k=1;k<=i;k++){
            cout<< "*";
        }
        
        cout<<endl;

        
    }
}
//OUTPUT:
      *
     **
    ***
   ****
  *****


=== Code Execution Successful ===
