#include<iostream>
using namespace std;
int main(){
    int i,j;
    char ch='A';
    int n=4;

    for(i=0;i<n;i++){
        
        for(j=0;j<=i;j++){
            cout<< ch ;
            ch++;
          
        }
       
        
        cout<<endl;

        
    }
}
//OUTPUT:
A
BC
DEF
GHIJ


=== Code Execution Successful ===
